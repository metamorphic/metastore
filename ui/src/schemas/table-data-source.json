      },
      "sourcingMethod": {
        "title": "Sourcing Method",
        "description": "Sourcing method",
        "type": "string"
      },
      "hostname": {
        "title": "Host Name",
        "description": "Server hostname",
        "type": "string"
      },
      "ipaddr": {
        "title": "Host IP Address",
        "description": "Server IP address",
        "type": "string"
      },
      "port": {
        "title": "Port",
        "description": "Server port number",
        "type": "string"
      },
      "firewallStatus": {
        "title": "Firewall Status",
